<%- include ('./header.ejs') %>

    <!-- END nav -->
    <section class="hero-wrap hero-wrap-2" style="background-image: url('images/7.jpg');" data-stellar-background-ratio="0.5">
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text align-items-end">
          <div class="col-md-9 ftco-animate pb-5">
          	<p class="breadcrumbs mb-2"><span class="mr-2"><a href="index.html">Home <i class="fa fa-chevron-right"></i></a></span> <span>Blog <i class="fa fa-chevron-right"></i></span></p>
            <h1 class="mb-0 bread">Our Blog</h1>
          </div>
        </div>
      </div>
    </section>

    <section class="ftco-section">
      <div class="container">
        <div class="row">


            <% for(i=0;i<result.length;i++) { %>
          <div class="col-md-4 d-flex ftco-animate">
            <div class="blog-entry align-self-stretch">
              <a href="/single-blog?id=<%=result[i].id%>" class="block-20 rounded" style="background-image: url('/images/<%=result[i].image%>');">
              </a>
              <div class="text mt-3">
              
                <h3 class="heading"><a href="/single-blog?id=<%=result[i].id%>"><%=result[i].name%></a></h3>
              </div>
            </div>
          </div>

          <% } %>
         
        </div>
        
      </div>
    </section>

  
    
<%- include ('./indexfooter.ejs') %>
  